<h1>My Uploaded Files</h1>
<table class="table table-striped">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Title</th>
      <th scope="col">Description</th>
      <th scope="col">File</th>
    </tr>
  </thead>
  <tbody>
    <% @uploads.each do |upload| %>
      <tr>
        <th><%=upload.id %></th>
        <td><%=upload.title %></td>
        <td><%=upload.description %></td>
        <td><%=upload.file.url %></td>
        <td><%=link_to "Generate Tiny Url", generate_upload_path(upload) %></td>
        <td><%=link_to "Share", upload_path(upload) %></td>
        <td><%=link_to "Delete", upload_path(upload), method: :delete %></td>
      </tr>
    <% end %>
  </tbody>
</table>
